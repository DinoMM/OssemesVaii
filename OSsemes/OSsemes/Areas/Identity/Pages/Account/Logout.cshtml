@page
@using OSsemes.Areas.Identity.Data
@using Microsoft.AspNetCore.Identity
@using OSsemes.Data.Database
@attribute [IgnoreAntiforgeryToken]
@inject SignInManager<IdentityUserOwn> SignManager

@functions {
    public async Task<IActionResult> OnGet() //(pomohol som si z internetu tutoriály/AI)
    {
        if (SignManager.IsSignedIn(User))
        {
            await SignManager.SignOutAsync();
        }
        return Redirect("~/");
    }
}
